{
  "name": "postcss-build",
  "version": "1.1.15",
  "description": "PostCSS cli build tool",
  "main": "index.js",
  "bin": {
    "postcssbuild": "./bin/postcssbuild"
  },
  "preferGlobal": true,
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-runner",
    "cli"
  ],
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "build:watch": "watch 'npm run build' ./src",
    "lint": "eslint --quiet ./src  || true",
    "lint:watch": "watch 'npm run lint' ./",
    "prepublish": "npm run build"
  },
  "author": "Otis Virginie",
  "repository": "git@github.com:otissv/postcss-build.git",
  "license": "MIT",
  "dependencies": {
    "chalk": "~1.1.3",
    "concat-files": "~0.1.0",
    "dedupe": "~2.0.3",
    "forEachAsync": "^3.0.0",
    "glob": "~7.0.3",
    "minimist": "~1.2.0",
    "node-notifier": "~4.6.0",
    "postcss": "~5.0.21",
    "shelljs": "~0.7.0",
    "watch": "^0.19.2"
  },
  "devDependencies": {
    "babel-cli": "~6.9.0",
    "babel-eslint": "~6.0.4",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-stage-2": "~6.5.0",
    "eslint": "~2.10.2",
    "eslint-config-standard": "~5.3.1",
    "eslint-plugin-promise": "~1.1.0",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.2"
  },
  "engines": {
    "node": "~4.4.5"
  }
}
